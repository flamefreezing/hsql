<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="org.freezing.naive.repository.BuildingRepository">


<select id="getBuildings" resultType="org.freezing.naive.dto.GetBuildingsOutDto">
		<![CDATA[
		    SELECT 
		    BUILDING.ID AS BUILDING_ID, 
		    BUILDING.NAME AS BUILDING_NAME 
		FROM 
		    BUILDING
		]]>
    </select>

</mapper>
